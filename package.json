{
  "name": "reason-cli",
  "version": "0.0.1",
  "devDependencies": {
    "turbo": "2.5.6",
    "typescript": "catalog:",
    "prettier": "catalog:"
  },
  "license": "Apache-2.0",
  "packageManager": "bun@1.3.2",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "typecheck": "turbo typecheck",
    "format": "prettier --write \"packages/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"packages/**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "turbo clean && rm -rf node_modules"
  },
  "type": "module",
  "workspaces": {
    "packages": ["packages/*"],
    "catalog": {
      "typescript": "5.8.2",
      "zod": "4.1.8",
      "@types/node": "22.13.9",
      "prettier": "3.4.2"
    }
  }
}
